    "presets": ["es2015", "react", "stage-3", "flow"],
    "env": {
        "development": {
            "presets": ["react-hmre"]
        }
    },
    "plugins": [
        "transform-flow-comments",
        "syntax-dynamic-import",
        "dynamic-import-webpack", ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "less" }]
    ]
}